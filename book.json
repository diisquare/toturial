{
  "title": "diisquare",
  "language": "zh-hans",
  "structure": {
    "readme": "introduction.md"
},
  "plugins":[
    "back-to-top-button",
    "highlight",
    "chapter-fold",
    "splitter",
    "tbfed-pagefooter",
    "flexible-alerts",
    "auto-scroll-table",
    "popup"
  ],
  "pluginsConfig": {
    "tbfed-pagefooter": {
        "copyright":"Copyright &copy DII WEB Group 2020",
        "modify_label": "该文件修订时间：",
        "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
    "flexible-alerts": {
      "style": "flat",
      "comment": {
        "label": "Comment",
        "icon": "fa fa-comments",
        "className": "info"
      }
    }
  }
}
